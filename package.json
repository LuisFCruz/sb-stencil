{
  "name": "sb-stencil",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sb-stencil/sb-stencil.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --watch ",
    "start:serve": "stencil build --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "serve": "npm-run-all --parallel start storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@stencil/core": "^1.12.2",
    "@stencil/sass": "^1.3.2",
    "@storybook/addon-a11y": "^6.0.0-rc",
    "@storybook/addon-controls": "^6.0.0-rc",
    "@storybook/addon-docs": "^6.0.0-rc",
    "@storybook/html": "^6.0.0-rc",
    "@types/jest": "24.9.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "jsx-dom": "^6.4.18",
    "npm-run-all": "^4.1.5",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT",
  "dependencies": {},
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
